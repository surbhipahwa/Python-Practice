x=[251431,567232,57255,414044,663517,958441,151134,300503,572226,784903,651259,806599,241467,803702]
y=[923950,610537,45440,744065,220402,416469,876585,919918,410760,205169,35629,797962,431685,731281]
for i in range(0,len(x)):
   sum=x[i] + y[i]
   i += 1
   print ('{}'.format(sum),end =" ")
   #end-parameter helps to change by default \n print feature of Python 3.0
